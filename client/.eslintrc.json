{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "max-len": [2, {
            "code": 140,
            "ignoreComments": true
        }],
        "require-jsdoc" : 0,
        "linebreak-style": 0
    }
}
